{
	"name": "luna-co-pilot",
	"displayName": "Luna Co-Pilot",
	"description": "An AI assistant sidebar for VS Code",
	"publisher": "lunabaseai",
	"version": "0.0.1",
	"icon": "assets/icons/icon.png",
	"galleryBanner": {
		"color": "#617A91",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.84.0",
		"node": ">=20.11.0"
	},
	"author": {
		"name": "Luna Base AI"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Luna-Base-AI/luna-co-pilot"
	},
	"homepage": "https://lunabase.ai",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"cline",
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama",
		"luna"
	],
	"activationEvents": [
		"onLanguage",
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"submenus": [
			{
				"id": "luna-co-pilot.contextMenu",
				"label": "Luna Co-Pilot"
			},
			{
				"id": "luna-co-pilot.terminalMenu",
				"label": "Luna Co-Pilot"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "luna-co-pilot-ActivityBar",
					"title": "Luna Co-Pilot",
					"icon": "assets/icons/icon.svg"
				}
			]
		},
		"views": {
			"luna-co-pilot-ActivityBar": [
				{
					"type": "webview",
					"id": "luna-co-pilot.SidebarProvider",
					"name": "",
					"icon": "assets/icons/icon.svg"
				}
			]
		},
		"commands": [
			{
				"command": "luna-co-pilot.plusButtonClicked",
				"title": "New Task",
				"icon": "$(add)"
			},
			{
				"command": "luna-co-pilot.mcpButtonClicked",
				"title": "MCP Servers",
				"icon": "$(server)"
			},
			{
				"command": "luna-co-pilot.promptsButtonClicked",
				"title": "Prompts",
				"icon": "$(notebook)"
			},
			{
				"command": "luna-co-pilot.historyButtonClicked",
				"title": "History",
				"icon": "$(history)"
			},
			{
				"command": "luna-co-pilot.popoutButtonClicked",
				"title": "Open in Editor",
				"icon": "$(link-external)"
			},
			{
				"command": "luna-co-pilot.settingsButtonClicked",
				"title": "Settings",
				"icon": "$(settings-gear)"
			},
			{
				"command": "luna-co-pilot.helpButtonClicked",
				"title": "Documentation",
				"icon": "$(question)"
			},
			{
				"command": "luna-co-pilot.openInNewTab",
				"title": "Open In New Tab",
				"category": "Luna Co-Pilot"
			},
			{
				"command": "luna-co-pilot.explainCode",
				"title": "Explain Code",
				"category": "Luna Co-Pilot"
			},
			{
				"command": "luna-co-pilot.fixCode",
				"title": "Fix Code",
				"category": "Luna Co-Pilot"
			},
			{
				"command": "luna-co-pilot.improveCode",
				"title": "Improve Code",
				"category": "Luna Co-Pilot"
			},
			{
				"command": "luna-co-pilot.addToContext",
				"title": "Add To Context",
				"category": "Luna Co-Pilot"
			},
			{
				"command": "luna-co-pilot.newTask",
				"title": "New Task",
				"category": "Luna Co-Pilot"
			},
			{
				"command": "luna-co-pilot.terminalAddToContext",
				"title": "Add Terminal Content to Context",
				"category": "Terminal"
			},
			{
				"command": "luna-co-pilot.terminalFixCommand",
				"title": "Fix This Command",
				"category": "Terminal"
			},
			{
				"command": "luna-co-pilot.terminalExplainCommand",
				"title": "Explain This Command",
				"category": "Terminal"
			},
			{
				"command": "luna-co-pilot.terminalFixCommandInCurrentTask",
				"title": "Fix This Command (Current Task)",
				"category": "Terminal"
			},
			{
				"command": "luna-co-pilot.terminalExplainCommandInCurrentTask",
				"title": "Explain This Command (Current Task)",
				"category": "Terminal"
			},
			{
				"command": "luna-co-pilot.setCustomStoragePath",
				"title": "Set Custom Storage Path",
				"category": "Luna Co-Pilot"
			},
			{
				"command": "luna-co-pilot.focusInput",
				"title": "Focus Input Field",
				"category": "Luna Co-Pilot"
			},
			{
				"command": "luna-co-pilot.acceptInput",
				"title": "Accept Input/Suggestion",
				"category": "Luna Co-Pilot"
			}
		],
		"menus": {
			"editor/context": [
				{
					"submenu": "luna-co-pilot.contextMenu",
					"group": "navigation"
				}
			],
			"luna-co-pilot.contextMenu": [
				{
					"command": "luna-co-pilot.addToContext",
					"group": "1_actions@1"
				},
				{
					"command": "luna-co-pilot.explainCode",
					"group": "1_actions@2"
				},
				{
					"command": "luna-co-pilot.fixCode",
					"group": "1_actions@3"
				},
				{
					"command": "luna-co-pilot.improveCode",
					"group": "1_actions@4"
				}
			],
			"terminal/context": [
				{
					"submenu": "luna-co-pilot.terminalMenu",
					"group": "navigation"
				}
			],
			"luna-co-pilot.terminalMenu": [
				{
					"command": "luna-co-pilot.terminalAddToContext",
					"group": "1_actions@1"
				},
				{
					"command": "luna-co-pilot.terminalFixCommand",
					"group": "1_actions@2"
				},
				{
					"command": "luna-co-pilot.terminalExplainCommand",
					"group": "1_actions@3"
				},
				{
					"command": "luna-co-pilot.terminalFixCommandInCurrentTask",
					"group": "1_actions@5"
				},
				{
					"command": "luna-co-pilot.terminalExplainCommandInCurrentTask",
					"group": "1_actions@6"
				}
			],
			"view/title": [
				{
					"command": "luna-co-pilot.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == luna-co-pilot.SidebarProvider"
				},
				{
					"command": "luna-co-pilot.promptsButtonClicked",
					"group": "navigation@2",
					"when": "view == luna-co-pilot.SidebarProvider"
				},
				{
					"command": "luna-co-pilot.mcpButtonClicked",
					"group": "navigation@3",
					"when": "view == luna-co-pilot.SidebarProvider"
				},
				{
					"command": "luna-co-pilot.historyButtonClicked",
					"group": "navigation@4",
					"when": "view == luna-co-pilot.SidebarProvider"
				},
				{
					"command": "luna-co-pilot.popoutButtonClicked",
					"group": "navigation@5",
					"when": "view == luna-co-pilot.SidebarProvider"
				},
				{
					"command": "luna-co-pilot.settingsButtonClicked",
					"group": "navigation@6",
					"when": "view == luna-co-pilot.SidebarProvider"
				},
				{
					"command": "luna-co-pilot.helpButtonClicked",
					"group": "navigation@7",
					"when": "view == luna-co-pilot.SidebarProvider"
				}
			],
			"editor/title": [
				{
					"command": "luna-co-pilot.plusButtonClicked",
					"group": "navigation@1",
					"when": "activeWebviewPanelId == luna-co-pilot.TabPanelProvider"
				},
				{
					"command": "luna-co-pilot.promptsButtonClicked",
					"group": "navigation@2",
					"when": "activeWebviewPanelId == luna-co-pilot.TabPanelProvider"
				},
				{
					"command": "luna-co-pilot.mcpButtonClicked",
					"group": "navigation@3",
					"when": "activeWebviewPanelId == luna-co-pilot.TabPanelProvider"
				},
				{
					"command": "luna-co-pilot.historyButtonClicked",
					"group": "navigation@4",
					"when": "activeWebviewPanelId == luna-co-pilot.TabPanelProvider"
				},
				{
					"command": "luna-co-pilot.popoutButtonClicked",
					"group": "navigation@5",
					"when": "activeWebviewPanelId == luna-co-pilot.TabPanelProvider"
				},
				{
					"command": "luna-co-pilot.settingsButtonClicked",
					"group": "navigation@6",
					"when": "activeWebviewPanelId == luna-co-pilot.TabPanelProvider"
				},
				{
					"command": "luna-co-pilot.helpButtonClicked",
					"group": "navigation@7",
					"when": "activeWebviewPanelId == luna-co-pilot.TabPanelProvider"
				}
			]
		},
		"configuration": {
			"title": "Luna Co-Pilot",
			"properties": {
				"luna-co-pilot.allowedCommands": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						"npm test",
						"npm install",
						"tsc",
						"git log",
						"git diff",
						"git show"
					],
					"description": "Commands that can be auto-executed when 'Always approve execute operations' is enabled"
				},
				"luna-co-pilot.vsCodeLmModelSelector": {
					"type": "object",
					"properties": {
						"vendor": {
							"type": "string",
							"description": "The vendor of the language model (e.g. copilot)"
						},
						"family": {
							"type": "string",
							"description": "The family of the language model (e.g. gpt-4)"
						}
					},
					"description": "Settings for VSCode Language Model API"
				},
				"luna-co-pilot.customStoragePath": {
					"type": "string",
					"default": "",
					"description": "Custom storage path. Leave empty to use the default location. Supports absolute paths (e.g. 'D:\\LunaStorage')"
				}
			}
		}
	},
	"scripts": {
		"install:all": "yarn && cd webview-ui && yarn",
		"dev": "npm-run-all --parallel dev:webview watch",
		"dev:webview": "cd webview-ui && yarn dev",
		"build:webview": "cd webview-ui && yarn build",
		"build": "yarn vsix",
		"build:esbuild": "node esbuild.js --production",
		"compile": "tsc -p . --outDir out && node esbuild.js",
		"lint": "npm-run-all -l -p lint:*",
		"lint:extension": "eslint src/**/*.ts",
		"lint:webview": "cd webview-ui && yarn lint",
		"check-types": "npm-run-all -l -p check-types:*",
		"check-types:extension": "tsc --noEmit",
		"check-types:webview": "cd webview-ui && yarn check-types",
		"package": "npm-run-all -l -p build:webview build:esbuild check-types lint",
		"pretest": "yarn compile",
		"test": "node scripts/run-tests.js",
		"test:extension": "jest -w=40%",
		"test:webview": "cd webview-ui && yarn test",
		"prepare": "husky",
		"vscode:prepublish": "yarn package",
		"watch": "npm-run-all -l -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"watch-tests": "tsc -p . -w --outDir out",
		"clean": "npm-run-all -l -p clean:*",
		"clean:extension": "rimraf bin dist out",
		"clean:webview": "cd webview-ui && yarn clean",
		"vscode-test": "npm-run-all -l -p vscode-test:*",
		"vscode-test:extension": "tsc -p . --outDir out && node esbuild.js",
		"vscode-test:webview": "cd webview-ui && yarn build"
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.10.2",
		"@anthropic-ai/sdk": "^0.37.0",
		"@anthropic-ai/vertex-sdk": "^0.7.0",
		"@aws-sdk/client-bedrock-runtime": "^3.779.0",
		"@google-cloud/vertexai": "^1.9.3",
		"@google/genai": "^0.9.0",
		"@mistralai/mistralai": "^1.3.6",
		"@modelcontextprotocol/sdk": "^1.7.0",
		"@types/clone-deep": "^4.0.4",
		"@types/pdf-parse": "^1.1.4",
		"@types/tmp": "^0.2.6",
		"@types/turndown": "^5.0.5",
		"@types/vscode": "^1.95.0",
		"@vscode/codicons": "^0.0.36",
		"axios": "^1.7.4",
		"cheerio": "^1.0.0",
		"chokidar": "^4.0.1",
		"clone-deep": "^4.0.1",
		"default-shell": "^2.2.0",
		"delay": "^6.0.0",
		"diff": "^5.2.0",
		"diff-match-patch": "^1.0.5",
		"fast-deep-equal": "^3.1.3",
		"fast-xml-parser": "^4.5.1",
		"fastest-levenshtein": "^1.0.16",
		"fzf": "^0.5.2",
		"get-folder-size": "^5.0.0",
		"i18next": "^24.2.2",
		"isbinaryfile": "^5.0.2",
		"js-tiktoken": "^1.0.19",
		"mammoth": "^1.8.0",
		"monaco-vscode-textmate-theme-converter": "^0.1.7",
		"node-ipc": "^12.0.0",
		"openai": "^4.78.1",
		"os-name": "^6.0.0",
		"p-wait-for": "^5.0.2",
		"pdf-parse": "^1.1.1",
		"pkce-challenge": "^4.1.0",
		"posthog-node": "^4.7.0",
		"pretty-bytes": "^6.1.1",
		"puppeteer-chromium-resolver": "^23.0.0",
		"puppeteer-core": "^23.4.0",
		"reconnecting-eventsource": "^1.6.4",
		"say": "^0.16.0",
		"serialize-error": "^11.0.3",
		"simple-git": "^3.27.0",
		"sound-play": "^1.1.0",
		"string-similarity": "^4.0.4",
		"strip-ansi": "^7.1.0",
